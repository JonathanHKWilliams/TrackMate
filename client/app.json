{
  "expo": {
    "name": "TrackMate",
    "slug": "trackmate",
    "scheme": "trackmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "assets/images/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trackmate.app"
    },
    "android": {
      "backgroundColor": "#000000",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.trackmate.app",
      "jsEngine": "hermes"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-web-browser",
      "expo-router",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "34cdb534-cc81-404c-b743-7a9784635ebf"
      }
    }
  }
}
